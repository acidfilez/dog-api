    pod 'PKHUD', '~> 5.0'
    pod 'Agrume', :git => 'https://github.com/JanGorman/Agrume.git'
    pod 'SwiftLint'
    pod 'EmptyKit'

  target 'dog-apiTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'dog-apiUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
